		case "18": ### - ttySessionCommencedV2c
			# SNMPv2
			# A person has successfully gained access to the ascii management port (telnet,
			# local management port, or HTTP). The username is included in the Trap.
				$ttySessionUserName = $1
				$ttySessionUserConnectionTypelook = "ttySessionUserConnectionType." + $2
				$ttySessionUserConnectionType = lookup($ttySessionUserConnectionTypelook,DragonWaveVarbindValTable)
				if(match($ttySessionUserConnectionType,"UNKNOWN")) {
					$ttySessionUserConnectionType = $2
				} else {
					$ttySessionUserConnectionType = $ttySessionUserConnectionType + " ( " + $2 + " )"
				}
			@AlertGroup = "dragonwave.airPair.apSnmpV2Notification.apSnmpV2Traps"
			@Agent = "dragonwave"
			@Summary = "ttySessionCommencedV2c"
			@Severity = 2
			@Type = 1
			@Identifier = @Node + @Summary
			#details($ttySessionUserName,$ttySessionUserConnectionType)

		case "19": ### - ttySessionTerminatedV2c
