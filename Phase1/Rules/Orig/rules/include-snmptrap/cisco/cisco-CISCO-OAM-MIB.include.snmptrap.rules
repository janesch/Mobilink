case ".1.3.6.1.4.1.9.10.15.2": ### Cisco OAM - Notifications from CISCO-OAM-MIB

    log(DEBUG, "<<<<< Entering... cisco-CISCO-OAM-MIB.include.snmptrap.rules >>>>>")

    @Agent = "Cisco-OAM"
    @Class = "40057"
    
    $OPTION_TypeFieldUsage = "3.6"
    
    switch($specific-trap)
    {
        case "1": ### oamLoopbackPingCompletionTrap

            ##########
            # $1 = oamLoopbackPingCompleted
            ##########

            $oamLoopbackPingCompleted = $1
            $oamLoopbackPingSerialNumber = extract($OID1, "\.([0-9]+)$")
            details($oamLoopbackPingCompleted)

            $OS_EventId = "SNMPTRAP-cisco-CISCO-OAM-MIB-oamLoopbackPingCompletionTrap"

            @AlertGroup = "OAM Loopback Ping Completion"
            @AlertKey = "oamLoopbackPingEntry." + $oamLoopbackPingSerialNumber
            @Summary = "Loopback Ping Completed for Sequence of OAM Loopback Cells  ( " + @AlertKey + " )"
            
            $DEFAULT_Severity = 1
            $DEFAULT_Type = 2
            $DEFAULT_ExpireTime = 0
            
            @Identifier = @Node + " " + @AlertKey + " " + @AlertGroup + " " + $DEFAULT_Type + " " + @Agent + " " + @Manager + " " + $specific-trap

        default:
        
            @Summary = "Unknown Specific Trap Number (" + $specific-trap + ") Received for Enterprise " + $enterprise
            @Severity = 1
            @Identifier = @Node + " " + @Agent + " " + @Manager + " " + $enterprise + " " + $generic-trap + " " + $specific-trap
            details($*)
    }

##########
# Handle Severity via Lookup.
##########

if(exists($SEV_KEY))
{
    [$OS_Severity,$OS_Type,$OS_ExpireTime] = lookup($SEV_KEY, cisco-CISCO-OAM-MIB_sev)
}
else
{
    [$OS_Severity,$OS_Type,$OS_ExpireTime] = lookup($OS_EventId, cisco-CISCO-OAM-MIB_sev)
}
include "$NC_RULES_HOME/include-common/AssignSev.include.common.rules"

##########
# End of Severity via Lookup.
##########

##########
# Enter "Advanced" and "User" includes.
##########

include "$NC_RULES_HOME/include-snmptrap/cisco/cisco-CISCO-OAM-MIB.adv.include.snmptrap.rules"
include "$NC_RULES_HOME/include-snmptrap/cisco/cisco-CISCO-OAM-MIB.user.include.snmptrap.rules"

##########
# End of "Advanced" and "User" includes.
##########

include "$NC_RULES_HOME/include-common/load_include.rules"

log(DEBUG, "<<<<< Leaving... cisco-CISCO-OAM-MIB.include.snmptrap.rules >>>>>")
