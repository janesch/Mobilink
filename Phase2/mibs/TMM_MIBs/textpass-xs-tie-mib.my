    ----------------------------------------------------------------------------
    --                                T R A P S                               --
    ----------------------------------------------------------------------------

    noUcs2UrlConfigured TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { deviceType }
        DESCRIPTION
            "No URL has been configured for HTTP requests triggered by UCS-2 
            encoded messages."
        ::= 1

    noGsm7UrlConfigured TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { deviceType }
        DESCRIPTION
            "No URL has been configured for HTTP requests triggered by GSM 
            default alphabet encoded messages."
        ::= 2

    trapInvalidUcs2Url TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { deviceType }
        DESCRIPTION
            "Invalid URL has been configured for HTTP requests triggered by
            UCS-2 encoded messages."
        ::= 3

    trapInvalidGsm7Url TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { deviceType }
        DESCRIPTION
            "Invalid URL has been configured for HTTP requests triggered by 
            GSM default alphabet encoded messages."
        ::= 4

    trapUcs2DnsLookupFailed TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { deviceType }
        DESCRIPTION
            "DNS resolver library could not resolv UCS2 URL domain name."
        ::= 5

    trapGsm7DnsLookupFailed TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { deviceType }
        DESCRIPTION
            "DNS resolver library could not resolv GSM7 URL domain name."
        ::= 6

    trapProxyNotConfigured TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { deviceType }
        DESCRIPTION
            "Proxy enabled and not configured correctly."
        ::= 7

    trapProxyLookupError TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { deviceType }
        DESCRIPTION
            "Proxy hostname DNS lookup failed."
        ::= 8



    addressDataFileUnavailable TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { xsTieAddressDataFilePath, xsTieLastAddressDataFileError, 
                    deviceType }
        DESCRIPTION
            "The specified data file cannot be read."
        ::= 11

    addressDataFileIncorrectlyFormatted TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { xsTieAddressDataFilePath, xsTieLastAddressDataFileError, 
                    deviceType }
        DESCRIPTION
            "The specified data file contains a line which is improperly 
            formed."
        ::= 12

    addressDataFileTooBig TRAP-TYPE
