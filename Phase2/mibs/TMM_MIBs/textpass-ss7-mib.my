    mtpTrunkTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF MtpTrunkEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "Table containing info about E1/T1 trunks."
        ::= { mtp 5 }
 
    mtpTrunkEntry OBJECT-TYPE
        SYNTAX  MtpTrunkEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "Entry of table containing info about E1/T1 trunks."
        INDEX   { mtpTrunkIndex }
        ::= { mtpTrunkTable 1 }

    MtpTrunkEntry ::=
        SEQUENCE {
            mtpTrunkIndex                         Gauge(1..6),
            mtpTrunkAdminState                    AdminState,
            mtpTrunkOperationalState              TrunkOperationalState,
            mtpTrunkType                          TrunkType,
            mtpTrunkLoopBack                      Boolean,
            mtpTrunkClockGeneration               Boolean,
            mtpTrunkBipolarViolations             Counter,
            mtpTrunkExcessiveZeroes               Counter,
            mtpTrunkFramingErrors                 Counter,           
            mtpTrunkCrc4Errors                    Counter,           
            mtpTrunkControlledSlips               Counter,
            mtpTrunkMultiframeLossOfFrame         Counter,
            mtpTrunkEBitErrors                    Counter,
            mtpTrunkSeverlyErroredFrames          Counter,
            mtpTrunkAtmScramblingEnabled          Boolean
        }

   mtpTrunkIndex OBJECT-TYPE
        SYNTAX  Gauge(1..16)
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "Index of entry in table containing info about E1/T1 trunks."
        ::= { mtpTrunkEntry 1 }

    mtpTrunkAdminState OBJECT-TYPE
        SYNTAX  AdminState
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Administrative state of entry in table containing info about the 
             E1/T1 trunks. Activating a trunk, deactivating a trunk or 
             removing an entry from table (and implicitly deactivating the 
             associated trunk) is accomplished by writing the administrative
             state."
        ::= { mtpTrunkEntry 2 }

    mtpTrunkOperationalState OBJECT-TYPE
        SYNTAX  TrunkOperationalState
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Operational state of E1/T1 trunk."
        ::= { mtpTrunkEntry 3 }

    mtpTrunkType OBJECT-TYPE
        SYNTAX  TrunkType
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Type of E1/T1 trunk. The default is E1 120 Ohm and CRC. Only
             modify-able if the entry's administrative state is 'inactive'."
        ::= { mtpTrunkEntry 4 }

    mtpTrunkLoopBack OBJECT-TYPE
        SYNTAX  Boolean
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Indicator to enable of disable loopback for E1/T1 trunk. When 
             loopback is enabled, data that the E1/T1 trunk receives from the
             SS7 network is sent back to the SS7 network without passing it to
             to TextPass. By default, loopback is disabled. Only modify-able 
             if the entry's administrative state is 'inactive'."
        ::= { mtpTrunkEntry 5 }

    mtpTrunkClockGeneration OBJECT-TYPE
        SYNTAX  Boolean
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Indicator to enable of disable clock generation of the trunk. When
             clock generation is enabled, TextPass generates a clock on Tx which
             is independent of the clock on Rx clock. When clock generation is
             disabled, TextPass derives the Tx clock from the Rx clock. By 
             default, clock generation is disabled. Only modify-able if the 
             entry's administrative state is 'inactive'."
        ::= { mtpTrunkEntry 6 }
        
    mtpTrunkBipolarViolations OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The Number of Bipolar Violations  (HDB3 or B8ZS) that occurred on the trunk since activation."          
        ::= { mtpTrunkEntry 7 }

    mtpTrunkExcessiveZeroes OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The Number of Excessive Zeroes that occurred on the trunk since activation."          
        ::= { mtpTrunkEntry 8 }

    mtpTrunkFramingErrors OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The Number of Framing Errors (FAS word errors) that occurred on the trunk since activation."          
        ::= { mtpTrunkEntry 9 }
    
    mtpTrunkCrc4Errors OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The Number of Frames with a wrong CRC4 that occurred on the trunk since boot up."          
        ::= { mtpTrunkEntry 10 }
    
    mtpTrunkControlledSlips OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The Number of Controlled Slips  (HDB3 or B8ZS) that occurred on the trunk since activation."          
        ::= { mtpTrunkEntry 11 }

    mtpTrunkMultiframeLossOfFrame OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The Number of multiframe loss-of-frame that occurred on the trunk since boot up."          
        ::= { mtpTrunkEntry 12 }

    mtpTrunkEBitErrors OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The Number of E1 E-bit errors that occurred on the trunk since activation."          
        ::= { mtpTrunkEntry 13 }

    mtpTrunkSeverlyErroredFrames OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "The Number of Severly Errored Frames that were recevied on the trunk since activation."          
        ::= { mtpTrunkEntry 14 }

    mtpTrunkAtmScramblingEnabled OBJECT-TYPE
        SYNTAX  Boolean
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Indicator specifying whether ATM scrambling should be enabled or not. Only applicable for HSL. 
            By default, ATM scrambling is enabled."          
        ::= { mtpTrunkEntry 31 }


