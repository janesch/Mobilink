		case "1": ### - lossOfSignalSetV1Trap
			# SNMPv1
			# Loss of signal set trap.
				$tribLineIndexlook = "tribLineIndex." + $1
				$tribLineIndex = lookup($tribLineIndexlook,DragonWaveVarbindValTable)
