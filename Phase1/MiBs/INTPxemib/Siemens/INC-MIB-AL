           tiGneType OBJECT-TYPE
               SYNTAX      DisplayString(SIZE(32))     
               MAX-ACCESS  accessible-for-notify
               STATUS      current
               DESCRIPTION 
                   "The type of the Network Element (NE).  E.g.:
                   'SCP', 'SMP', 'CSC', 'ICP', 'Router', 'Hub',
                   'Firewall', ..."
               ::= { tiGneEntry 4 }

        --
        -- The Performance Monitoring (PM) Counter table.
	-- Currently used for alarm reporting only. 
        --

           tiPmCounterTable OBJECT-TYPE
               SYNTAX     SEQUENCE OF TiPmCounterEntry
               MAX-ACCESS not-accessible
               STATUS     current
               DESCRIPTION
               "The Performance Monitoring (PM) Counter table. 
                Currently used for alarm reporting only."
        ::= { tiInc 1020 }
   
           tiPmCounterEntry OBJECT-TYPE
               SYNTAX     TiPmCounterEntry
               MAX-ACCESS not-accessible
               STATUS     current
               DESCRIPTION
               "An entry in the PM counter table containing information 
	        about one PM counter."
               INDEX { tiPmCounterId, tiPmCounterSource }
               ::= { tiPmCounterTable 1 }
   
           TiPmCounterEntry ::= SEQUENCE {
               tiPmCounterRowStatus     RowStatus,
               tiPmCounterId            DisplayString,
               tiPmCounterLabel         DisplayString,
               tiPmCounterSource        DisplayString
                   }
      
           tiPmCounterRowStatus OBJECT-TYPE
               SYNTAX     RowStatus
               MAX-ACCESS read-create
               STATUS     current
               DESCRIPTION
               "The status column is used for creating, modifying, and 
                deleting instances of the columnar objects in the GNE
                table."
               DEFVAL { active }        
               ::= { tiPmCounterEntry 1 }
   
           tiPmCounterId OBJECT-TYPE
               SYNTAX     DisplayString
               MAX-ACCESS accessible-for-notify
               STATUS     current
               DESCRIPTION
               "The identifier assigned to the PM counter. 
	        At the human interface and at the remote OS interface 
		the PM counter will be identified by this name. 
		This auxiliary variable is used to form the instance-
                identifiers of the columnar objects in the PM counter 
		table."
               ::= { tiPmCounterEntry 2 }
   
           tiPmCounterLabel OBJECT-TYPE
               SYNTAX     DisplayString
               MAX-ACCESS read-only
               STATUS     current
               DESCRIPTION
               "The label assigned to the PM counter. 
	        At the human interface and at the remote OS interface 
		the PM counter will be identified by this label (see 
		tiPmCounterId). 
		This read-only variable is intended to display the 
		label of the PM counter to the user."
               ::= { tiPmCounterEntry 3 }

           tiPmCounterSource OBJECT-TYPE
               SYNTAX     DisplayString
               MAX-ACCESS read-only
               STATUS     current
               DESCRIPTION
               "The source (host) name assigned to the PM counter. 
	        At the human interface and at the remote OS interface 
		the PM counter will be identified by this label (see 
		tiPmCounterId). 
		May be an empty string if the counter is not specific
		for a single source.
		This read-only variable is intended to display the 
		source name of the PM counter to the user."
               ::= { tiPmCounterEntry 4 }

