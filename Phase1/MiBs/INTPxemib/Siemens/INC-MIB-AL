
INC-MIB-AL DEFINITIONS ::= BEGIN

        IMPORTS
                  enterprises, MODULE-IDENTITY, OBJECT-IDENTITY,
                  OBJECT-TYPE, NOTIFICATION-TYPE       FROM SNMPv2-SMI

                  TEXTUAL-CONVENTION, DisplayString,   
                  RowStatus                            FROM SNMPv2-TC

                  tiInc, tiIncTraps, tiIncTrapVars,
                  TiBoolean, inModules                 FROM INC-MIB-C;

    -- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    --
    -- This module defines a part of the SNMPv2C MIB definition of
    -- the INCommander and the @vantage Commander as seen by an OSS
    -- (Operations Support System).
    --
    -- The scope of this module is the reporting of alarms to the OSS.
    --

    -- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    --
    -- Specific Textual Conventions :
    --

        -- currently none

    -- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    --
    -- Module Identity :
    --

    inIncMibAL MODULE-IDENTITY  
        LAST-UPDATED "0703161700Z"
        ORGANIZATION "Siemens AG"
        CONTACT-INFO "TMN/IN-Team
                      eMail: klaus.schmidtke@siemens.com"
        DESCRIPTION
                    "This MIB module describes the managed objecttypes
                     of the INCommander and the @vantage Commander which 
                     will be used currently only for the INC/AC internal
                     alarming as reporting and faulty object.

                     In addition, the notification types for the alarms 
                     reported by the INC/AC to an Operations Support System
                     (OSS) are defined in this MIB module."

        REVISION    "0703161700Z"
        DESCRIPTION "added value indeterminate(9999) to object type
		     tiAlarmEventTypeId."

        REVISION    "0504121600Z"
        DESCRIPTION "Corrected SIZE constraints of the object types
		     tiNum...Alarms."

        REVISION    "0411301200Z"
        DESCRIPTION "Added IMPORT statement for OID enterprises."

        REVISION    "0304091600Z"
        DESCRIPTION "Added SIZE constraints to the object types
		     tiAlarmDescription, tiAlarmSymbNEname."

        REVISION    "0202281400Z"
        DESCRIPTION "Changed the syntax clause of object types
                     tiAlarmNotificationID and tiAlarmSyncNotifID
		     to INTEGER([1|0]..2147483647)"

        REVISION    "0111161200Z"
        DESCRIPTION "Added tiLogfileTable for alarming purposes only."

        REVISION    "0012150900Z"
        DESCRIPTION "Changed the syntax clause of object type
                     tiAlarmNotificationID
		     from INTEGER(...) to Counter32(...)"

        REVISION    "0001311600Z"
        DESCRIPTION "Changed the syntax clause of the object types
                     tiAlarmDescription and tiAlarmSourceName
		     from OCTET STRING to DisplayString"

        REVISION    "9911171400Z"
        DESCRIPTION "Added tiPmCounterTable for alarming purposes only."

        REVISION    "9906211600Z"
        DESCRIPTION "Adaptations to TeMIP MIB loader.
                     Adaption of tiAlarmSeverity and tiMaxSeverity 
                     to ITU-T Rec. X.721. 
                     Adaption of tiAlarmEventTypeId to X.733"

        REVISION    "9905171200Z"
        DESCRIPTION "Added object types tiAlarmTransferID and 
                     tiAlarmSyncNotifID.
                     Renaming in EventInfoEntry.
                     tiReqSeqNumbers renamed to tiReqTransferIDs. 
                     tiStartTime renamed to tiAlarmSyncStartTime."

        REVISION    "9904291700Z"
        DESCRIPTION "naming of sequence number object types clarified"

        REVISION    "9903251200Z"
        DESCRIPTION "Added object type group tiIncAlarmControl.
                     Added object tiAlarmNotificationID."

        REVISION    "9811091600Z"
        DESCRIPTION "Added object types tiAlarmNEtype and tiGneType"

        REVISION    "9810161200Z"
        DESCRIPTION "- Added tiIcpTable, tiGneTable
                     - using inModules"

        REVISION    "9806181200Z"
        DESCRIPTION "- minor changes to meet RFC1902
                     - MIB Private Syntax Extensions for the DEC TeMIP 
                       Operation System added"
                     
        REVISION    "9711071200Z"
        DESCRIPTION "enhancements for phase 2:
                     Additional NOTIFICATION-TYPE macro
                     instanziations for alarm summary reports."

        ::= { inModules 2 } 

    -- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    --
    -- used foreign object types :
    --
        -- Definitions derived from the file "$OV_CONF/snmpmib" 
        -- for EMANATE tooling only :
        hp                OBJECT IDENTIFIER ::= { enterprises 11 }
        nm                OBJECT IDENTIFIER ::= { hp 2 }
        openView          OBJECT IDENTIFIER ::= { nm 17 }
        hpOpenView        OBJECT IDENTIFIER ::= { openView 1 }
        openViewTrapVars  OBJECT IDENTIFIER ::= { openView 2 }

        openViewTraps     OBJECT IDENTIFIER ::= { hpOpenView 0 }

        ovEvtNodeDown     OBJECT IDENTIFIER ::= { openViewTraps 58916865 }
        ovEvtIfUp         OBJECT IDENTIFIER ::= { openViewTraps 58916866 }

        --
        -- openViewSourceName OID: 1.3.6.1.4.1.11.2.17.2.2.0, Type: OCTET STRING
        --
        -- refer to manpage trapd.conf(4)
        --

        -- Defined in module 'HP-UNIX'. Definition derived from the file 
        -- "$OV_CONF/snmpmib" for EMANATE tooling only :
        -- the OpenView trap variables Group
        -- These object ID's cannot be retrieved from the SNMP Agent, but
        -- instead document the objects sent with OpenView SNMP traps
 
        openViewSourceId OBJECT-TYPE
                SYNTAX        INTEGER(-2147483647..2147483647)
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                "The identifier of the software generating the trap/event.
                 This number is used by HP OpenView software when it sends
                 an event to the OpenView event system.  It identifies
                 which software component sent the event.
                 
                 This object cannot be retrieved from the SNMP agent."
                ::= { openViewTrapVars 1 }

        openViewSourceName OBJECT-TYPE
                SYNTAX  OCTET STRING(SIZE (0..255))
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                "The source of the event (may not be the machine upon
                 which the event was generated).  This string is used
                 by HP OpenView software when it sends an event.  It
                 identifies for which source (node) the event is generated.
 
                 This object cannot be retrieved from the SNMP agent."

                ::= { openViewTrapVars 2 }

        openViewObjectId OBJECT-TYPE
                SYNTAX  OCTET STRING(SIZE (0..255))
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                "The OpenView object identifier associated with the
                 source of the trap/event.

                 This object cannot be retrieved from the SNMP agent."
                ::= { openViewTrapVars 3 }

        openViewData OBJECT-TYPE
                SYNTAX  OCTET STRING(SIZE (0..255))
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                "Any miscellaneous data sent with an OpenView trap/event.

                 This object cannot be retrieved from the SNMP agent."
                ::= { openViewTrapVars 4 }

        openViewSeverity OBJECT-TYPE
                SYNTAX  OCTET STRING(SIZE (0..255))
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                "The OpenView event severity associated with the trap/event.

                 This object cannot be retrieved from the SNMP agent."
                ::= { openViewTrapVars 5 }

        openViewCategory OBJECT-TYPE
                SYNTAX  OCTET STRING(SIZE (0..255))
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                "The OpenView event category associated with the trap/event.

                 This object cannot be retrieved from the SNMP agent."
                ::= { openViewTrapVars 6 }

        openViewFilter OBJECT-TYPE
                SYNTAX  OCTET STRING(SIZE (0..255))
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                "The event filter for an application connecting to the
                 OpenView event system.

                 This object cannot be retrieved from the SNMP agent."
                ::= { openViewTrapVars 7 }

        openViewEntity OBJECT-TYPE
                SYNTAX  OCTET STRING(SIZE (0..255))
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                "The entity (string name) of an application connecting
                 to the OpenView event system.

                 This object cannot be retrieved from the SNMP agent."
                ::= { openViewTrapVars 8 }

        openViewAddress OBJECT-TYPE
                SYNTAX  OCTET STRING(SIZE (0..255))
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                "The IP address of the node from where an application
                 is connecting to the OpenView event system.

                 This object cannot be retrieved from the SNMP agent."
                ::= { openViewTrapVars 9 }

        openViewPid OBJECT-TYPE
                SYNTAX  OCTET STRING(SIZE (0..255))
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                "The process ID of an application connecting to the
                 OpenView event system.

                 This object cannot be retrieved from the SNMP agent."
                ::= { openViewTrapVars 10 }

    -- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

        tiIncAlarmReporting OBJECT IDENTIFIER ::= { tiInc 2 } 

    -- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    --
    -- object types used as FaultyOID in INC internal alarms :
    --

        tiIncServer OBJECT-IDENTITY
                STATUS current
                DESCRIPTION
                        "The authoritative identity of the INC server."
                ::= { tiInc 1000 }

        tiIncConsole OBJECT-IDENTITY
                STATUS current
                DESCRIPTION
                        "The authoritative identity of the INC console."
                ::= { tiInc 1001 }

        --
        -- The OMT table. Currently used for alarm reporting only. :
        --

           tiOmtTable OBJECT-TYPE
               SYNTAX     SEQUENCE OF TiOmtEntry
               MAX-ACCESS not-accessible
               STATUS     current
               DESCRIPTION
               "The OMT table. Currently used for alarm reporting only."
        ::= { tiInc 1002 }
   
           tiOmtEntry OBJECT-TYPE
               SYNTAX     TiOmtEntry
               MAX-ACCESS not-accessible
               STATUS     current
               DESCRIPTION
               "An entry in the OMT table containing information about one OMT."
               INDEX { tiOmtSymbolicName }
               ::= { tiOmtTable 1 }
   
           TiOmtEntry ::= SEQUENCE {
               tiOmtRowStatus     RowStatus,
               tiOmtSymbolicName  DisplayString,
               tiOmtName          DisplayString 
                   }
      
           tiOmtRowStatus OBJECT-TYPE
               SYNTAX     RowStatus
               MAX-ACCESS read-create
               STATUS     current
               DESCRIPTION
               "The status column is used for creating, modifying, and 
                deleting instances of the columnar objects in the OMT
                table."
               DEFVAL { active }        
               ::= { tiOmtEntry 1 }
   
           tiOmtSymbolicName OBJECT-TYPE
               SYNTAX     DisplayString
               MAX-ACCESS accessible-for-notify
               STATUS     current
               DESCRIPTION
               "The symbolic name assigned to the OMT. At the human interface
                and at the remote OS interface the OMT will be identified by
                this name. This auxiliary variable is used to form the instance-
                identifiers of the columnar objects in the OMT table."
               ::= { tiOmtEntry 2 }
   
           tiOmtName OBJECT-TYPE
               SYNTAX     DisplayString
               MAX-ACCESS read-only
               STATUS     current
               DESCRIPTION
               "The symbolic name assigned to the OMT. At the human interface
                and at the remote OS interface the OMT will be identified by
                this name (see tiOmtSymbolicName). This read-only variable is
                intened to display the symbolic name of the OMT to the user."
               ::= { tiOmtEntry 3 }


        tiIncAlPrinter OBJECT-IDENTITY
               STATUS current
               DESCRIPTION
               "The authoritative identity of the INC Alarm Printer."
               ::= { tiInc 1003 }

        --
        -- The CSC table. Currently used for alarm reporting only. :
