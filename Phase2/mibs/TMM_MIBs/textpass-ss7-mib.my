            suaAspRemoteAddressSctpRecvHeartbeatAckCounter Counter,
            suaAspRemoteAddressSctpRoundTripTime   SctpRoundTripTime,
            suaAspRemoteAddressSctpRetransmissionTimeout SctpRetransmitTimeout,
            suaAspRemoteAddressSctpSmoothedLinkQuality SctpSmoothedLinkQuality
