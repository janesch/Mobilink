        VARIABLES { xsTieTextDataFilePath, xsTieLastTextDataFileError, 
                    deviceType }
        DESCRIPTION
            "The specified data file contains lines which are improperly 
            formed."
        ::= 22

    textDataFileTooBig TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { xsTieTextDataFilePath, xsTieLastTextDataFileError, 
                    deviceType }
        DESCRIPTION
            "The specified data file contains too many entries (> 1.000)."
        ::= 23

    textDataFileEmpty TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { xsTieTextDataFilePath, xsTieLastTextDataFileError, 
                    deviceType }
        DESCRIPTION
            "The specified data file is empty."
