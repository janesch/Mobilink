			# Yellow Alarm Clear trap. The Yellow Alarm cleared on the
			# indicated tributary.
				$lineIndexlook = "lineIndex." + $1
				$lineIndex = lookup($lineIndexlook,DragonWaveVarbindValTable)
				if(match($lineIndex,"UNKNOWN")) {
					$lineIndex = $1
				} else {
					$lineIndex = $lineIndex + " ( " + $1 + " )"
				}
				$tribIndexlook = "tribIndex." + $2
				$tribIndex = lookup($tribIndexlook,DragonWaveVarbindValTable)
				if(match($tribIndex,"UNKNOWN")) {
					$tribIndex = $2
				} else {
					$tribIndex = $tribIndex + " ( " + $2 + " )"
				}
			@AlertGroup = "dragonwave.sdu.apx.apxProducts.apxDS3.traps.apxSnmpV2Traps"
			@Agent = "dragonwave"
			@Summary = "tribYellowAlarmClearV2c"
			@Severity = 2
			@Type = 1
			@Identifier = @Node + @Summary
			#details($lineIndex,$tribIndex)

		case "15": ### - tribUnRunAlarmSetV2c
			# SNMPv2
			# Buffer underrun Alarm trap set.
				$lineIndexlook = "lineIndex." + $1
				$lineIndex = lookup($lineIndexlook,DragonWaveVarbindValTable)
				if(match($lineIndex,"UNKNOWN")) {
					$lineIndex = $1
				} else {
					$lineIndex = $lineIndex + " ( " + $1 + " )"
				}
				$tribIndexlook = "tribIndex." + $2
				$tribIndex = lookup($tribIndexlook,DragonWaveVarbindValTable)
				if(match($tribIndex,"UNKNOWN")) {
					$tribIndex = $2
				} else {
					$tribIndex = $tribIndex + " ( " + $2 + " )"
				}
			@AlertGroup = "dragonwave.sdu.apx.apxProducts.apxDS3.traps.apxSnmpV2Traps"
			@Agent = "dragonwave"
			@Summary = "tribUnRunAlarmSetV2c"
			@Severity = 2
			@Type = 1
			@Identifier = @Node + @Summary
			#details($lineIndex,$tribIndex)

		case "16": ### - tribUnRunAlarmClearV2c
			# SNMPv2
			# Buffer underrun Alarm trap cleared.
				$lineIndexlook = "lineIndex." + $1
				$lineIndex = lookup($lineIndexlook,DragonWaveVarbindValTable)
				if(match($lineIndex,"UNKNOWN")) {
					$lineIndex = $1
				} else {
					$lineIndex = $lineIndex + " ( " + $1 + " )"
				}
				$tribIndexlook = "tribIndex." + $2
				$tribIndex = lookup($tribIndexlook,DragonWaveVarbindValTable)
				if(match($tribIndex,"UNKNOWN")) {
					$tribIndex = $2
				} else {
					$tribIndex = $tribIndex + " ( " + $2 + " )"
				}
			@AlertGroup = "dragonwave.sdu.apx.apxProducts.apxDS3.traps.apxSnmpV2Traps"
			@Agent = "dragonwave"
			@Summary = "tribUnRunAlarmClearV2c"
			@Severity = 2
			@Type = 1
			@Identifier = @Node + @Summary
			#details($lineIndex,$tribIndex)

		case "17": ### - tribOvRunAlarmSetV2c
			# SNMPv2
			# Buffer overrun Alarm trap set.
				$lineIndexlook = "lineIndex." + $1
				$lineIndex = lookup($lineIndexlook,DragonWaveVarbindValTable)
				if(match($lineIndex,"UNKNOWN")) {
					$lineIndex = $1
				} else {
					$lineIndex = $lineIndex + " ( " + $1 + " )"
				}
				$tribIndexlook = "tribIndex." + $2
				$tribIndex = lookup($tribIndexlook,DragonWaveVarbindValTable)
				if(match($tribIndex,"UNKNOWN")) {
					$tribIndex = $2
				} else {
					$tribIndex = $tribIndex + " ( " + $2 + " )"
				}
			@AlertGroup = "dragonwave.sdu.apx.apxProducts.apxDS3.traps.apxSnmpV2Traps"
			@Agent = "dragonwave"
			@Summary = "tribOvRunAlarmSetV2c"
			@Severity = 2
			@Type = 1
			@Identifier = @Node + @Summary
			#details($lineIndex,$tribIndex)

		case "18": ### - tribOvRunAlarmClearV2c
			# SNMPv2
			# Buffer overrun Alarm trap cleared.
				$lineIndexlook = "lineIndex." + $1
				$lineIndex = lookup($lineIndexlook,DragonWaveVarbindValTable)
				if(match($lineIndex,"UNKNOWN")) {
					$lineIndex = $1
				} else {
					$lineIndex = $lineIndex + " ( " + $1 + " )"
				}
				$tribIndexlook = "tribIndex." + $2
				$tribIndex = lookup($tribIndexlook,DragonWaveVarbindValTable)
				if(match($tribIndex,"UNKNOWN")) {
					$tribIndex = $2
				} else {
					$tribIndex = $tribIndex + " ( " + $2 + " )"
				}
			@AlertGroup = "dragonwave.sdu.apx.apxProducts.apxDS3.traps.apxSnmpV2Traps"
			@Agent = "dragonwave"
			@Summary = "tribOvRunAlarmClearV2c"
			@Severity = 2
			@Type = 1
			@Identifier = @Node + @Summary
			#details($lineIndex,$tribIndex)

		case "19": ### - bitsAlarmSetV2c
			# SNMPv2
			# Bits Alarm trap set.
			@AlertGroup = "dragonwave.sdu.apx.apxProducts.apxDS3.traps.apxSnmpV2Traps"
			@Agent = "dragonwave"
			@Summary = "bitsAlarmSetV2c"
			@Severity = 2
			@Type = 1
			@Identifier = @Node + @Summary
		case "20": ### - bitsAlarmClearV2c
			# SNMPv2
			# BITS alarm trap cleared.
			@AlertGroup = "dragonwave.sdu.apx.apxProducts.apxDS3.traps.apxSnmpV2Traps"
			@Agent = "dragonwave"
			@Summary = "bitsAlarmClearV2c"
			@Severity = 2
			@Type = 1
			@Identifier = @Node + @Summary
		case "21": ### - fanAlarmSetV2c
			# SNMPv2
			# Fan Alarm trap set.
			@AlertGroup = "dragonwave.sdu.apx.apxProducts.apxDS3.traps.apxSnmpV2Traps"
			@Agent = "dragonwave"
			@Summary = "fanAlarmSetV2c"
			@Severity = 2
			@Type = 1
			@Identifier = @Node + @Summary
		case "22": ### - fanAlarmClearV2c
			# SNMPv2
			# Fan alarm trap cleared.
			@AlertGroup = "dragonwave.sdu.apx.apxProducts.apxDS3.traps.apxSnmpV2Traps"
			@Agent = "dragonwave"
			@Summary = "fanAlarmClearV2c"
			@Severity = 2
			@Type = 1
			@Identifier = @Node + @Summary
		default:
			@Summary = "Unknown specific trap number (" + $specific-trap + ") received for enterprise " + $enterprise
			@Identifier = @Node + $enterprise + $generic-trap + $specific-trap + @Agent + @Manager
			@Severity = 2
			details($*)
	}

