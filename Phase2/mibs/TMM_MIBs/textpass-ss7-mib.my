        SYNTAX  AdminState
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Administrative state of the M3UA ASP."
        ::= { m3uaAspEntry 2 }

    m3uaAspOperationalState OBJECT-TYPE
        SYNTAX  M3uaAspOperationalState
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Operational state of the M3UA ASP."
        ::= { m3uaAspEntry 3 }

    m3uaAspName OBJECT-TYPE
        SYNTAX  NameString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Name of M3UA ASP."
        ::= { m3uaAspEntry 4 }

    m3uaAspAspIdentifier OBJECT-TYPE
        SYNTAX  AspIdentifier
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
