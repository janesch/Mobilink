case ".1.3.6.1.4.1.2011.5.22.1": ### Huawei - Notifications from HUAWEI-8021PAE-MIB

    log(DEBUG, "<<<<< Entering... huawei-HUAWEI-8021PAE-MIB.include.snmptrap.rules >>>>>")

    @Agent = "Huawei AR- and S-Series"
    @Class = "40579"

    $OPTION_TypeFieldUsage = "3.6"

    switch($specific-trap)
    {
        case "1": ### supplicantproxycheck

            ##########
            # $1 = proxycheckVlanId - (no DESCRIPTION in HUAWEI-8021PAE-MIB)
            # $2 = proxycheckPortName - (no DESCRIPTION in HUAWEI-8021PAE-MIB)
            # $3 = proxycheckMacAddr - (no DESCRIPTION in HUAWEI-8021PAE-MIB)
            # $4 = proxycheckIpaddr - (no DESCRIPTION in HUAWEI-8021PAE-MIB)
            # $5 = proxycheckUsrName - (no DESCRIPTION in HUAWEI-8021PAE-MIB)
            ##########

            $proxycheckVlanId = $1
            $proxycheckPortName = $2
            $proxycheckMacAddr = $3
            $proxycheckIpaddr = $4
            $proxycheckUsrName = $5
            details($proxycheckVlanId,$proxycheckPortName,
                    $proxycheckMacAddr,$proxycheckIpaddr,
                    $proxycheckUsrName)

            $OS_EventId = "SNMPTRAP-huawei-HUAWEI-8021PAE-MIB-supplicantproxycheck"

            @AlertGroup = "Proxy Check"
            @AlertKey = "VLAN-Id: " + $1 + ", PortName: " + $2
            @Summary = "Proxy Check Notification - see details  ( " + @AlertKey + " )"

            $DEFAULT_Severity = 2
            $DEFAULT_Type = 13
            $DEFAULT_ExpireTime = 1800

            @Identifier = @Node + " " + @AlertKey + " " + @AlertGroup + " " + $DEFAULT_Type + " " + @Agent + " " + @Manager + " " + $specific-trap

        default:

            @Summary = "Unknown Specific Trap Number (" + $specific-trap + ") Received for Enterprise " + $enterprise
            @Severity = 1
            @Identifier = @Node + " " + @Agent + " " + @Manager + " " + $enterprise + " " + $generic-trap + " " + $specific-trap
            details($*)
    }

#########
# Handle Severity via Lookup.
##########

if(exists($SEV_KEY))
{
    [$OS_Severity,$OS_Type,$OS_ExpireTime] = lookup($SEV_KEY, huawei-HUAWEI-8021PAE-MIB_sev)
}
else
{
    [$OS_Severity,$OS_Type,$OS_ExpireTime] = lookup($OS_EventId, huawei-HUAWEI-8021PAE-MIB_sev)
}
include "$NC_RULES_HOME/include-common/AssignSev.include.common.rules"

##########
# End of Severity via Lookup.
##########

##########
# Enter "Advanced" and "User" includes.
##########

include "$NC_RULES_HOME/include-snmptrap/huawei/huawei-HUAWEI-8021PAE-MIB.adv.include.snmptrap.rules"
include "$NC_RULES_HOME/include-snmptrap/huawei/huawei-HUAWEI-8021PAE-MIB.user.include.snmptrap.rules"

##########
# End of "Advanced" and "User" includes.
##########

log(DEBUG, "<<<<< Leaving... huawei-HUAWEI-8021PAE-MIB.include.snmptrap.rules >>>>>")
