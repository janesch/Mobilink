log(DEBUG, "<<<<< Entering... cisco-CISCO-ITP-SCCP-MIB.adv.include.snmptrap.rules >>>>>")

switch($specific-trap)
{
    case "1": ### cItpSccpGttMapStateChange

        $OS_X733EventType = 1
        $OS_X733ProbableCause = 1009
        $OS_X733SpecificProb = "cItpSccpGttMapStateChange"
        $OS_OsiLayer = 0

        $OS_LocalPriObj = "cItpSccpGttMapTableEntry." + $cItpSccpGttMapPc + "." + $cItpSccpGttMapSsn
        $OS_LocalRootObj = $OS_LocalPriObj
        $VAR_RelateLRO2LPO = 1
        $VAR_RelateLRO2LSO = 0
        $VAR_RelateLSO2LPO = 0

    default:
}

log(DEBUG, "<<<<< Leaving... cisco-CISCO-ITP-SCCP-MIB.adv.include.snmptrap.rules >>>>>")
