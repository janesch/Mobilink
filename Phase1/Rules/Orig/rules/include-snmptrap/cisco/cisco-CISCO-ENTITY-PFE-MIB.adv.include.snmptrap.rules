log(DEBUG, "<<<<< Entering... cisco-CISCO-ENTITY-PFE-MIB.adv.include.snmptrap.rules >>>>>")

switch($specific-trap)
{
    case "1": ### cePfeHistThldEvent

        $OS_X733EventType = 1
        $OS_X733ProbableCause = 1008
        $OS_X733SpecificProb = "cePfeHistThldEvent"
        $OS_OsiLayer = 0

        $OS_LocalPriObj = "entPhysicalEntry." + $1 
        $OS_LocalRootObj = $OS_LocalPriObj
        $VAR_RelateLRO2LPO = 1
        $VAR_RelateLRO2LSO = 0
        $VAR_RelateLSO2LPO = 0

    case "2": ### cePfeHistRestartEvent

        $OS_X733EventType = 1
        $OS_X733ProbableCause = 1008
        $OS_X733SpecificProb = "cePfeHistRestartEvent"
        $OS_OsiLayer = 0

        $OS_LocalPriObj = "entPhysicalEntry." + $1 
        $OS_LocalRootObj = $OS_LocalPriObj
        $VAR_RelateLRO2LPO = 1
        $VAR_RelateLRO2LSO = 0
        $VAR_RelateLSO2LPO = 0

    default:
}

log(DEBUG, "<<<<< Leaving... cisco-CISCO-ENTITY-PFE-MIB.adv.include.snmptrap.rules >>>>>")
