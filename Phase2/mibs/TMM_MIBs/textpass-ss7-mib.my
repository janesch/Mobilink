    m3uaSgpSctpSendBuffer OBJECT-TYPE
        SYNTAX  BufferSize
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Requested size of the local send buffer in bytes. 
            The default value is 0, which means to use the OS default. 
            Note that the OS may have a maximum size, 
            which can not be exceeded without changing system wide settings.
            This option has no effect on Solaris 8."
        ::= { m3uaSgpEntry 141 }

    m3uaSgpSctpSendBufferActual OBJECT-TYPE
        SYNTAX  BufferSize
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Actual size of the local send buffer in bytes. 
            Some rounding may be applied compared to the requested setting,
            and a ceiling may be enforced by the OS.
            This option has no effect on Solaris 8."
        ::= { m3uaSgpEntry 142 }

    m3uaSgpSctpSendLoWat OBJECT-TYPE
        SYNTAX  BufferSize
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Requested low water mark of the local send buffer in bytes. 
            The default value is 0, which means to use the OS default. 
            This value has to be lower than the send buffer size. 
            Usually a value between 10% and 25% is recommended.
            This option has no effect on Solaris 8."
        ::= { m3uaSgpEntry 143 }

    m3uaSgpSctpSendLoWatActual OBJECT-TYPE
        SYNTAX  BufferSize
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Actual low water mark of the local send buffer in bytes. 
            This option has no effect on Solaris 8."
        ::= { m3uaSgpEntry 144 }

    m3uaSgpSctpRecvBuffer OBJECT-TYPE
        SYNTAX  BufferSize
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Requested size of the local recv buffer in bytes. 
            The default value is 0, which means to use the OS default. 
            Note that the OS may have a maximum size, 
            which can not be exceeded without changing system wide settings.
            This option has no effect on Solaris 8."
        ::= { m3uaSgpEntry 145 }

    m3uaSgpSctpRecvBufferActual OBJECT-TYPE
        SYNTAX  BufferSize
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Actual size of the local recv buffer in bytes. 
            Some rounding may be applied compared to the requested setting,
            and a ceiling may be enforced by the OS.
            This option has no effect on Solaris 8."
        ::= { m3uaSgpEntry 146 }

    m3uaSgpSctpRecvLoWat OBJECT-TYPE
        SYNTAX  BufferSize
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Requested low water mark of the local receive buffer in bytes. 
            The default value is 0, which means to use the OS default. 
            This value has to be lower than the receive buffer size. 
            Usually a value between 10% and 25% is recommended.
            This option has no effect on Solaris 8."
        ::= { m3uaSgpEntry 147 }

    m3uaSgpSctpRecvLoWatActual OBJECT-TYPE
        SYNTAX  BufferSize
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Actual low water mark of the local receive buffer in bytes. 
            This option has no effect on Solaris 8."
        ::= { m3uaSgpEntry 148 }

    m3uaSgpSctpNoDelay OBJECT-TYPE
        SYNTAX  Boolean
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Requested setting of the NO DELAY flag. 
            Setting this flags disables the Nagle algorithm.
            The Nagle algorithm may reduce the network load on 
            high latency low throughput connections by delaying and
            bundling small messages into one network packet.
            The messages are only delayed until the aknowledgement
            of the previous packet is received 
            (or until a full packet can be send).
            Disabling the Nagle algorithm may reduce latency slightly on
            high latency low throughput connections, at the expense
            of an increased network load. 
            However, disabling the Nagle algorithm is no guarantee that
            no bundling is performed.
            The default is false, which means to use the Nagle algorithm.
            This option has no effect on Solaris 8,
            because Solaris 8 does not support bundling of SCTP messages."
        ::= { m3uaSgpEntry 149 }

    m3uaSgpSctpNoDelayActual OBJECT-TYPE
        SYNTAX  Boolean
        ACCESS  read-only
        STATUS  mandatory
        DESCRIPTION
            "Actual setting of the NO DELAY flag.
            This option has no effect on Solaris 8,
            because Solaris 8 does not support bundling of SCTP messages."
        ::= { m3uaSgpEntry 150 }

    -- the M3UA SGP remote IP address table

    m3uaSgpRemoteAddressTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF M3uaSgpRemoteAddressEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "Table containing info about IP addresses of remote M3UA Application 
            Servers Processes (ASP's)."
        ::= { m3ua 12 }

    m3uaSgpRemoteAddressEntry OBJECT-TYPE
        SYNTAX  M3uaSgpRemoteAddressEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "Entry of table containing info about IP addresses of remote M3UA 
            Signalling Gateways Processes (ASP's)."
        INDEX   { m3uaSgpIndex, m3uaSgpRemoteAddressIndex }
        ::= { m3uaSgpRemoteAddressTable 1 }

    M3uaSgpRemoteAddressEntry ::=
        SEQUENCE {
            m3uaSgpRemoteAddressIndex               M3uaRemoteAddressTableIndex,
            m3uaSgpRemoteAddressAdminState          AdminState,
            m3uaSgpRemoteAddressOperationalState    RemoteAddressOperationalState,
            m3uaSgpRemoteAddressSpec                IpAddressSpec,
            m3uaSgpRemoteAddressSctpErrorCounter    Counter,
            m3uaSgpRemoteAddressSctpSentHeartbeatCounter Counter,
            m3uaSgpRemoteAddressSctpRecvHeartbeatCounter Counter,
            m3uaSgpRemoteAddressSctpSentHeartbeatAckCounter Counter,
            m3uaSgpRemoteAddressSctpRecvHeartbeatAckCounter Counter,
            m3uaSgpRemoteAddressSctpRoundTripTime   SctpRoundTripTime,
            m3uaSgpRemoteAddressSctpRetransmissionTimeout SctpRetransmitTimeout,
            m3uaSgpRemoteAddressSctpSmoothedLinkQuality SctpSmoothedLinkQuality
        }    

    m3uaSgpRemoteAddressIndex OBJECT-TYPE
        SYNTAX  M3uaRemoteAddressTableIndex
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Index of entry in table with IP addresses for remote M3UA SGP."
        ::= { m3uaSgpRemoteAddressEntry 1 }

    m3uaSgpRemoteAddressAdminState OBJECT-TYPE
        SYNTAX  AdminState
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Administrative state of IP address of remote M3UA SGP."
        ::= { m3uaSgpRemoteAddressEntry 2 }

    m3uaSgpRemoteAddressOperationalState OBJECT-TYPE
        SYNTAX  RemoteAddressOperationalState
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Operational state of IP address of remote M3UA SGP."
        ::= { m3uaSgpRemoteAddressEntry 3 }

    m3uaSgpRemoteAddressSpec OBJECT-TYPE
        SYNTAX  IpAddressSpec
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "IPv4 or IPv6 specification for IP address of remote M3UA SGP."
        ::= { m3uaSgpRemoteAddressEntry 11 }

    m3uaSgpRemoteAddressSctpErrorCounter OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Counter specifying number of SCTP errors for IP address of
            remote M3UA SGP."
        ::= { m3uaSgpRemoteAddressEntry 21 }

    m3uaSgpRemoteAddressSctpSentHeartbeatCounter OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Counter specifying the number of HEARTBEAT chunks sent to IP 
            address of remote M3UA SGP."
        ::= { m3uaSgpRemoteAddressEntry 31 }

    m3uaSgpRemoteAddressSctpRecvHeartbeatCounter OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Counter specifying the number of HEARTBEAT chunks received from IP 
            address of remote M3UA SGP."
        ::= { m3uaSgpRemoteAddressEntry 32 }

    m3uaSgpRemoteAddressSctpSentHeartbeatAckCounter OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Counter specifying the number of HEARTBEAT ACK chunks sent to IP
            address of remote M3UA SGP."
        ::= { m3uaSgpRemoteAddressEntry 33 }

    m3uaSgpRemoteAddressSctpRecvHeartbeatAckCounter OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Counter specifying the number of HEARTBEAT ACK chunks received from
            IP address of remote M3UA SGP."
        ::= { m3uaSgpRemoteAddressEntry 34 }

    m3uaSgpRemoteAddressSctpRoundTripTime OBJECT-TYPE
        SYNTAX  SctpRoundTripTime
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Current value of the round trip time (RTT) for IP address of remote 
            M3UA SGP."
        ::= { m3uaSgpRemoteAddressEntry 41 }

    m3uaSgpRemoteAddressSctpRetransmissionTimeout OBJECT-TYPE
        SYNTAX  SctpRetransmitTimeout
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Current value of the retransmission timeout (RTO) for the IP  
            address of remote M3UA SGP."
        ::= { m3uaSgpRemoteAddressEntry 42 }

    m3uaSgpRemoteAddressSctpSmoothedLinkQuality OBJECT-TYPE
        SYNTAX  SctpSmoothedLinkQuality
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            ""
        ::= { m3uaSgpRemoteAddressEntry 43 }

