	# Adding Link details into the Summary Field
	if (regmatch(@AlertKey, ".*SubLink No\.=.*"))
	{
		$linkDetails= extract(@AlertKey, ".*(SubLink No\.=.*$)")
                @Summary = @Summary + " :: " + $linkDetails
