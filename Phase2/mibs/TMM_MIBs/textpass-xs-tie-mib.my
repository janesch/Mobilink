        VARIABLES { xsTieAddressDataFilePath, xsTieLastAddressDataFileError, 
                    deviceType }
        DESCRIPTION
            "The specified data file contains too many entries (> 1.000.000).
            "
        ::= 13

    addressDataFileEmpty TRAP-TYPE
        ENTERPRISE xsTie
        VARIABLES { xsTieAddressDataFilePath, xsTieLastAddressDataFileError, 
                    deviceType }
        DESCRIPTION
            "The specified data file is empty."
        ::= 14

    textDataFileUnavailable TRAP-TYPE
