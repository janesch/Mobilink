    -- the MTP subsystem
 
    mtp OBJECT IDENTIFIER ::= { textpass 11 }

    -- the MTP addresses group

    mtpAddresses OBJECT IDENTIFIER ::= { mtp 1 }
       
    mtpAddrNetworkIndicator OBJECT-TYPE
        SYNTAX  NetworkIndicator
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Indicator specifying the type of SS.7 network
             Values:
             0 - international
             1 - spare (international use only)
             2 - national
             3 - reserved for national use.
             Default value is 2."             
        ::= { mtpAddresses 1 }
        
    mtpAddrActualPointCode OBJECT-TYPE
        SYNTAX  PointCode
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Point code used to address this specific TextPass device and 
             formatted as commonly done in ITU-T.
             Note that this point code can be changed/viewed by this object as 
             well as the mtpAddrActualPointCodeString object."
        ::= { mtpAddresses 2 }

    mtpAddrVirtualPointCode OBJECT-TYPE
        SYNTAX  PointCode
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Point code used to address a platform consisting of a group of 
             TextPass devices and formatted as commonly done in ITU-T.
             Note that this point code can be changed/viewed by this object as 
             well as the mtpAddrVirtualPointCodeString object."
        ::= { mtpAddresses 3 }
        
    mtpAddrActualPointCodeString OBJECT-TYPE
        SYNTAX  PointCodeString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Point code used to address this specific TextPass device and 
             formatted as commonly done in ANSI.
             Note that this point code can be changed/viewed by this object as 
             well as the mtpAddrActualPointCode object."
        ::= { mtpAddresses 4 }

    mtpAddrVirtualPointCodeString OBJECT-TYPE
        SYNTAX  PointCodeString
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Point code used to address a platform consisting of a group of 
             TextPass devices and formatted as commonly done in ANSI.
             Note that this point code can be changed/viewed by this object as 
             well as the mtpAddrVirtualPointCode object."
        ::= { mtpAddresses 5 }
        
