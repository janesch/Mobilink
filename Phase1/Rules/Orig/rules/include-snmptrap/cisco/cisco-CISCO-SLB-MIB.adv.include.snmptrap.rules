log(DEBUG, "<<<<< Entering... cisco-CISCO-SLB-MIB.adv.include.snmptrap.rules >>>>>")

switch($specific-trap)
{
    case "1": ### ciscoSlbVirtualStateChange

        $OS_X733EventType = 1
        $OS_X733ProbableCause = 1008
        $OS_X733SpecificProb = "ciscoSlbVirtualStateChange"
        $OS_OsiLayer = 3
        
        $OS_LocalPriObj = "slbVirtualServerTableEntry." + $slbEntity + "." + $slbVirtualServerName_OctetString
        $OS_LocalRootObj = "slbStatsTableEntry" + $slbEntity
        $VAR_RelateLRO2LPO = 3
        $VAR_RelateLRO2LSO = 0
        $VAR_RelateLSO2LPO = 0

    case "2": ### ciscoSlbRealStateChange

        $OS_X733EventType = 1
        $OS_X733ProbableCause = 1008
        $OS_X733SpecificProb = "ciscoSlbVirtualStateChange"
        $OS_OsiLayer = 3
        
        $OS_LocalPriObj = "slbRealTableEntry." + $slbEntity + "." + $slbRealServerFarmName_OctetString + "." + $slbRealIpAddress + "." + $slbRealPort
        $OS_LocalRootObj = "slbStatsTableEntry" + $slbEntity
        $VAR_RelateLRO2LPO = 3
        $VAR_RelateLRO2LSO = 0
        $VAR_RelateLSO2LPO = 0

    default:
}

log(DEBUG, "<<<<< Leaving... cisco-CISCO-SLB-MIB.adv.include.snmptrap.rules >>>>>")
