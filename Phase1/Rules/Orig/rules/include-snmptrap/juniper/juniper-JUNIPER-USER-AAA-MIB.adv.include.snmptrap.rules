log(DEBUG, "<<<<< Entering... juniper-JUNIPER-USER-AAA-MIB.adv.include.snmptrap.rules >>>>>")

switch($specific-trap)
{
    case "1": ### jnxAccessAuthServiceUp

        $OS_X733EventType = 1
        $OS_X733ProbableCause = 1008
        $OS_X733SpecificProb = "jnxAccessAuthServiceUp"
        $OS_OsiLayer = 0

    case "2": ### jnxAccessAuthServiceDown

        $OS_X733EventType = 1
        $OS_X733ProbableCause = 1008
        $OS_X733SpecificProb = "jnxAccessAuthServiceDown"
        $OS_OsiLayer = 0

    case "3": ### jnxAccessAuthServerDisabled

        $OS_X733EventType = 1
        $OS_X733ProbableCause = 1008
        $OS_X733SpecificProb = "jnxAccessAuthServerDisabled"
        $OS_OsiLayer = 0

        $OS_LocalPriObj = "jnxUserAAAServerName." + $1 
        $OS_LocalRootObj = $OS_LocalPriObj
        $VAR_RelateLRO2LPO = 1
        $VAR_RelateLRO2LSO = 0
        $VAR_RelateLSO2LPO = 0

    case "4": ### jnxAccessAuthServerEnabled

        $OS_X733EventType = 1
        $OS_X733ProbableCause = 1008
        $OS_X733SpecificProb = "jnxAccessAuthServerEnabled"
        $OS_OsiLayer = 0

        $OS_LocalPriObj = "jnxUserAAAServerName." + $1 
        $OS_LocalRootObj = $OS_LocalPriObj
        $VAR_RelateLRO2LPO = 1
        $VAR_RelateLRO2LSO = 0
        $VAR_RelateLSO2LPO = 0

    default:
}

log(DEBUG, "<<<<< Leaving... juniper-JUNIPER-USER-AAA-MIB.adv.include.snmptrap.rules >>>>>")
