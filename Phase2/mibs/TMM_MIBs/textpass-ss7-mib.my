    -- the M3UA ASP remote IP address table

    m3uaAspRemoteAddressTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF M3uaAspRemoteAddressEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "Table containing info about IP addresses of remote M3UA Application 
            Servers Processes (ASP's)."
        ::= { m3ua 22 }

    m3uaAspRemoteAddressEntry OBJECT-TYPE
        SYNTAX  M3uaAspRemoteAddressEntry
        ACCESS  not-accessible
        STATUS  mandatory
        DESCRIPTION
            "Entry of table containing info about IP addresses of remote M3UA 
            Application Servers Processes (ASP's)."
        INDEX   { m3uaAspIndex, m3uaAspRemoteAddressIndex }
        ::= { m3uaAspRemoteAddressTable 1 }

    M3uaAspRemoteAddressEntry ::=
        SEQUENCE {
            m3uaAspRemoteAddressIndex               M3uaRemoteAddressTableIndex,
            m3uaAspRemoteAddressAdminState          AdminState,
            m3uaAspRemoteAddressOperationalState    RemoteAddressOperationalState,
            m3uaAspRemoteAddressSpec                IpAddressSpec,
            m3uaAspRemoteAddressSctpErrorCounter    Counter,
            m3uaAspRemoteAddressSctpSentHeartbeatCounter Counter,
            m3uaAspRemoteAddressSctpRecvHeartbeatCounter Counter,
            m3uaAspRemoteAddressSctpSentHeartbeatAckCounter Counter,
            m3uaAspRemoteAddressSctpRecvHeartbeatAckCounter Counter,
            m3uaAspRemoteAddressSctpRoundTripTime   SctpRoundTripTime,
            m3uaAspRemoteAddressSctpRetransmissionTimeout SctpRetransmitTimeout,
            m3uaAspRemoteAddressSctpSmoothedLinkQuality SctpSmoothedLinkQuality
        }    

    m3uaAspRemoteAddressIndex OBJECT-TYPE
        SYNTAX  M3uaRemoteAddressTableIndex
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Index of entry in table with IP addresses for remote M3UA ASP."
        ::= { m3uaAspRemoteAddressEntry 1 }

    m3uaAspRemoteAddressAdminState OBJECT-TYPE
        SYNTAX  AdminState
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Administrative state of IP address of remote M3UA ASP."
        ::= { m3uaAspRemoteAddressEntry 2 }

    m3uaAspRemoteAddressOperationalState OBJECT-TYPE
        SYNTAX  RemoteAddressOperationalState
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Operational state of IP address of remote M3UA ASP."
        ::= { m3uaAspRemoteAddressEntry 3 }

    m3uaAspRemoteAddressSpec OBJECT-TYPE
        SYNTAX  IpAddressSpec
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "IPv4 or IPv6 specification for IP address of remote M3UA ASP."
        ::= { m3uaAspRemoteAddressEntry 11 }

    m3uaAspRemoteAddressSctpErrorCounter OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Counter specifying number of SCTP errors for IP address of
            remote M3UA ASP."
        ::= { m3uaAspRemoteAddressEntry 21 }

    m3uaAspRemoteAddressSctpSentHeartbeatCounter OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Counter specifying the number of HEARTBEAT chunks sent to IP 
            address of remote M3UA ASP."
        ::= { m3uaAspRemoteAddressEntry 31 }

    m3uaAspRemoteAddressSctpRecvHeartbeatCounter OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Counter specifying the number of HEARTBEAT chunks received from IP 
            address of remote M3UA ASP."
        ::= { m3uaAspRemoteAddressEntry 32 }

    m3uaAspRemoteAddressSctpSentHeartbeatAckCounter OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Counter specifying the number of HEARTBEAT ACK chunks sent to IP
            address of remote M3UA ASP."
        ::= { m3uaAspRemoteAddressEntry 33 }

    m3uaAspRemoteAddressSctpRecvHeartbeatAckCounter OBJECT-TYPE
        SYNTAX  Counter
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Counter specifying the number of HEARTBEAT ACK chunks received from
            IP address of remote M3UA ASP."
        ::= { m3uaAspRemoteAddressEntry 34 }

    m3uaAspRemoteAddressSctpRoundTripTime OBJECT-TYPE
        SYNTAX  SctpRoundTripTime
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Current value of the round trip time (RTT) for IP address of remote 
            M3UA ASP."
        ::= { m3uaAspRemoteAddressEntry 41 }

    m3uaAspRemoteAddressSctpRetransmissionTimeout OBJECT-TYPE
        SYNTAX  SctpRetransmitTimeout
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            "Current value of the retransmission timeout (RTO) for the IP  
            address of remote M3UA ASP."
        ::= { m3uaAspRemoteAddressEntry 42 }

    m3uaAspRemoteAddressSctpSmoothedLinkQuality OBJECT-TYPE
        SYNTAX  SctpSmoothedLinkQuality
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
            ""
        ::= { m3uaAspRemoteAddressEntry 43 }


