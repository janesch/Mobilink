			@Summary = "rtpEvtTrapMajor: " + $rtpEvtFaultyObject + " " + $rtpEvtReportingObject + " " + $rtpEvtSequenceNumberIndex + " " + $rtpEvtSet + " " + $rtpEvtNum
			
