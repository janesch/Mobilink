    -- +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
    --
    -- Definitions regarding Alarm Surveillance function 'Report Alarm' :
    -- 

        --
        -- Object Types :
        -- 

        tiIncAlarmTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF AlarmTableEntry
                MAX-ACCESS  not-accessible
                STATUS  current
                DESCRIPTION
                      "A list of tiAlarmTable entries."
        ::= { tiIncAlarmReporting 1 }

        tiIncAlarmTableEntry OBJECT-TYPE
                    SYNTAX     AlarmTableEntry
                    MAX-ACCESS not-accessible
                    STATUS     current
                DESCRIPTION 
                        "The information about a alarm that has occured 
                         inbetween a NE ( e.g. SCP ) or INC."
                INDEX { tiAlarmFaultyObject, tiAlarmErrorID }
        ::= { tiIncAlarmTable 1 }

            AlarmTableEntry ::= SEQUENCE {
                tiAlarmDateTime        DisplayString,
                tiAlarmReportingObject OBJECT IDENTIFIER,
                tiAlarmFaultyObject    OBJECT IDENTIFIER,
                tiAlarmEventTypeId     INTEGER,
                tiAlarmSeverity        INTEGER,
                tiAlarmErrorID         INTEGER,
                tiAlarmEndKey          INTEGER,
                tiAlarmDescription     OCTET STRING,
                tiAlarmSequenceNumber  INTEGER,
                tiAlarmSourceName      DisplayString,
                tiAlarmSymbNEname      DisplayString,
                tiAlarmNEtype          DisplayString,
                tiAlarmTransferID      INTEGER,
                tiAlarmNotificationID  INTEGER
        }

        tiAlarmDateTime OBJECT-TYPE
                SYNTAX DisplayString(SIZE(19))     
                    MAX-ACCESS accessible-for-notify
                STATUS current
                DESCRIPTION
                 "Represents the occurrence date and time of an alarm as 
                  display string in the format as defined by the 
                  ASN.1 standard data type GeneralizedTime 
                  ( used format: YYYYMMDDHHMMSS[+-]HHMM,
                  e.g.: 19960322123000+0100 ). Refer to IS ISO/IEC 8824 
                  respectively ITU-T Recommendation X.208"
            ::= { tiIncAlarmTableEntry 1 }

        tiAlarmReportingObject OBJECT-TYPE
                    SYNTAX     OBJECT IDENTIFIER
                    MAX-ACCESS accessible-for-notify
                    STATUS     current
                    DESCRIPTION
                   "Object Id of the alarm reporting object according 
                    to the SNMP object model defined for the NE."
            ::= { tiIncAlarmTableEntry 2 }

        tiAlarmFaultyObject OBJECT-TYPE
                    SYNTAX     OBJECT IDENTIFIER
                    MAX-ACCESS accessible-for-notify
                    STATUS     current
                    DESCRIPTION
                   "Object Id of the faulty object according to the SNMP
                    object model defined for the NE. Could be identical 
                    with the Object Id of the alarm reporting object."
            ::= { tiIncAlarmTableEntry 3 }

        tiAlarmEventTypeId OBJECT-TYPE
                SYNTAX INTEGER        {
                                communicationsAlarm (2),
                                environmentalAlarm (3),
                                equipmentAlarm (4),
                                processingErrorAlarm (10),
                                qualityOfServiceAlarm (11),
                        	indeterminate(9999)
                                }
                MAX-ACCESS accessible-for-notify
                STATUS current
                DESCRIPTION
                 "Event Type Id as integer. This field specifies the 
                  type of the alarm being reported. 
                  The value range conforms to the ITU-T Recommendation 
                  X.733."
            ::= { tiIncAlarmTableEntry 4 }

        tiAlarmSeverity OBJECT-TYPE
                SYNTAX INTEGER {
                        indeterminate(0),
                        -- used when it is not possible to assign 
                        -- one of the following values :
                        critical(1), 
                        major(2), 
                        minor(3), 
                        warning(4), 
                        cleared(5)
                    }
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                 "The perceived severity of an alarm as integer 
                  conforming to the ITU-T Recommendation X.721.
                  Alarm cleared messages are sent with the value 
                  cleared."
            ::= { tiIncAlarmTableEntry 5 }

        tiAlarmErrorID OBJECT-TYPE
                SYNTAX INTEGER (1..4294967295)     
                MAX-ACCESS accessible-for-notify
                STATUS current
                DESCRIPTION
                 "Error Id as integer. This Error Id is specific to 
                  the general alarm source type (e.g. SCP, INC)."
            ::= { tiIncAlarmTableEntry 6 }
  
        tiAlarmEndKey OBJECT-TYPE
                SYNTAX     TiBoolean
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                 "End Key of an alarm as integer. It indicates whether 
                  an alarm cleared message is expected ( value label 
                  true ) or not ( value label false )."
            ::= { tiIncAlarmTableEntry 7 }

        tiAlarmDescription OBJECT-TYPE
                SYNTAX  OCTET STRING(SIZE (0..255))
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                 "Represents the description of an alarm.
                  Additional free form text as octett string
                  in ISO Latin-1 format."
            ::= { tiIncAlarmTableEntry 8 }

        tiAlarmSequenceNumber OBJECT-TYPE
                SYNTAX        INTEGER (1..2147483647)
                MAX-ACCESS  accessible-for-notify
                STATUS  deprecated
                DESCRIPTION 
                  "The unique event sequence number.
                   Will be incremented by one for each new event.
                   Could wrap from 2147483647 to 1.
                   In case of INCommander internal alarms always 1.

                   This field is provided for compatibility with 
                   earlier versions of the INCommander.
                   Obsoleted by tiAlarmTransferID."

        ::= { tiIncAlarmTableEntry 9 }

        tiAlarmSourceName OBJECT-TYPE
                SYNTAX DisplayString
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION 
                "The source of the event (may not be the machine upon
                 which the event was generated)."
        ::= { tiIncAlarmTableEntry 10 }

        tiAlarmSymbNEname OBJECT-TYPE
                SYNTAX	DisplayString(SIZE (1..64))
                MAX-ACCESS  accessible-for-notify
                STATUS  current
                DESCRIPTION 
                        "The symbolic Network Element ( NE ) name.
                        Normally it is different from the source name."
        ::= { tiIncAlarmTableEntry 11 }

        tiAlarmNEtype OBJECT-TYPE
                SYNTAX      DisplayString(SIZE(32))     
                MAX-ACCESS  accessible-for-notify
                STATUS      current
                DESCRIPTION 
                   "The type of the Network Element (NE).  E.g.:
                   'SCP', 'SMP', 'CSC', 'ICP', 'Router', 'Hub',
                   'Firewall', ..."
        ::= { tiIncAlarmTableEntry 12 }

        tiAlarmNotificationID OBJECT-TYPE
                SYNTAX        INTEGER (1..2147483647)
                MAX-ACCESS  accessible-for-notify
                STATUS  current
                DESCRIPTION 
                   "The notification id unique for a whole INCommander
                    management area and is common for all configured OS's.

                    It will be incremented by one for each new event
                    received from any configured network element ( 
                    including the INCommander itself ) and could wrap 
                    from 2147483647 to 1.

                    Gaps in the notification identifier of consecutive
                    alarm SNMP traps received at the OS site are normal
                    because of the separate EFD processing for each OS." 
        ::= { tiIncAlarmTableEntry 13 }

        tiAlarmTransferID OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION
                   "The OS specific unique transfer identifier.
                    Will be incremented by one for each new event
                    sent to an OS.
                    Could wrap from 2147483647 to 1.

                    Gaps in the transfer identifier of consecutive
                    alarm SNMP traps received at the OS site indicates 
                    the lost of SNMP traps sent from the INCommander to 
                    the OS."
        ::= { tiIncAlarmTableEntry 14 }

        -- object types for alarm summary reports :

        tiIsConnectionReliable OBJECT-TYPE
                SYNTAX     TiBoolean
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                 "Alarm Summary Report:
                  Is the host responding to requests ?"
        ::= { tiIncAlarmReporting 2 }

        tiMaxSeverity OBJECT-TYPE
                SYNTAX INTEGER {
                        indeterminate(0),
                        -- used when it is not possible to assign 
                        -- one of the following values :
                        critical(1), 
                        major(2), 
                        minor(3), 
                        warning(4), 
                        cleared(5)
                       }
                MAX-ACCESS accessible-for-notify
                STATUS     current
                DESCRIPTION
                 "Alarm Summary Report:
                  Highest alarm severity of the host specified
                  with the trap object tiAlarmSourceName.0"
        ::= { tiIncAlarmReporting 3 }

        tiNumCriticalAlarms OBJECT-TYPE
                SYNTAX INTEGER (0..2147483647)
                MAX-ACCESS accessible-for-notify
                STATUS current
                DESCRIPTION
                 "Alarm Summary Report:
                  Number of open critical alarms."
        ::= { tiIncAlarmReporting 4 }

        tiNumMajorAlarms OBJECT-TYPE
                SYNTAX INTEGER (0..2147483647)
                MAX-ACCESS accessible-for-notify
                STATUS current
                DESCRIPTION
                 "Alarm Summary Report:
                  Number of open major alarms."
        ::= { tiIncAlarmReporting 5 }

        tiNumMinorAlarms OBJECT-TYPE
                SYNTAX INTEGER (0..2147483647)
                MAX-ACCESS accessible-for-notify
                STATUS current
                DESCRIPTION
                 "Alarm Summary Report:
                  Number of open minor alarms."
        ::= { tiIncAlarmReporting 6 }

        tiNumWarnings OBJECT-TYPE
                SYNTAX INTEGER (0..2147483647)
                MAX-ACCESS accessible-for-notify
                STATUS current
                DESCRIPTION
                 "Alarm Summary Report:
                  Number of open warnings."
        ::= { tiIncAlarmReporting 7 }

        tiNumNormalEvents OBJECT-TYPE
                SYNTAX INTEGER (0..2147483647)
                MAX-ACCESS accessible-for-notify
                STATUS current
                DESCRIPTION
                 "Alarm Summary Report:
                  Number of open events with severity normal."
        ::= { tiIncAlarmReporting 8 }

        tiNumUnknownEvents OBJECT-TYPE
                SYNTAX INTEGER (0..2147483647)
                MAX-ACCESS accessible-for-notify
                STATUS current
                DESCRIPTION
                 "Alarm Summary Report:
                  Number of open events with unknown severity."
        ::= { tiIncAlarmReporting 9 }


        --
        -- object types for the group Alarm Control :
        --

        tiIncAlarmControl OBJECT-IDENTITY
                STATUS current
                DESCRIPTION
                "The authoritative identity of the INC alarm control."
                ::= { tiIncAlarmReporting 100 }

        tiOsName OBJECT-TYPE
                SYNTAX DisplayString(SIZE(0..32))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                 "Write access: symbolic name of the requesting OS in the 
                  INCommander object database. Used in conjunction with
                  other selection objects ( e.g. tiAlarmSyncStartTime.0 ).
                  Read access: empty string."
        ::= { tiIncAlarmControl 1 }

        tiAlarmSyncStartTime OBJECT-TYPE
                SYNTAX DisplayString(SIZE(12))
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                 "Alarm synchronization :

                  This object could be passed in a SNMP Set Request 
                  along with the object tiOsName.0 in order to initiate
                  the transmission of alarms in the central INCommander 
                  Active Alarm Database as SNMP traps to the requesting OS.

                  Write access: 
                  Time stamp ( local time ) with the format 
                  YYYYMMDDhhmm whereas
                    YYYY = year; value range: 1970... 2099
                    MM   = month; value range: 01...12
                    DD   = day of month; value range: 01...31
                    hh   = hour; value range: 00...23
                    mm   = minute; value range: 00...59

                  The value 197001010000 could be used for requesting 
                  the whole actual content of the INCommander Active 
                  Alarm Database.
                   
                  Read access: empty string."
        ::= { tiIncAlarmControl 10 }

        tiClearNotificationIds OBJECT-TYPE
                SYNTAX DisplayString
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                 "Alarm synchronization :

                  This object could be passed in a SNMP Set Request
		  along with the object tiOsName.0 in order to initiate
		  the clearing of alarms from the central INCommander
		  Active Alarm Database.

                  Write access: notification id(s) of the event(s)
                  to be cleared from the INCommander active alarm
                  database. Syntax:

                    tiClearNotificationIds := item 
                            | tiClearNotificationIds ',' item

                    item   := number
                              | range

                    number := digits
                     
                    range  := number '-' number
                     
                  Examples: 7771,7774 
                            1233-1235
                            1234
                            1230,1233-1235
                            

                  Read access: empty string."

        ::= { tiIncAlarmControl 11 }

        tiAlarmSyncNotifID OBJECT-TYPE
                SYNTAX     INTEGER (0..2147483647)
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                 "Alarm synchronization :

                  This object could be passed in a SNMP Set Request 
                  along with the object tiOsName.0 in order to initiate
                  the transmission of alarms in the central INCommander 
                  Active Alarm Database as SNMP traps to the requesting OS.

                  Write access: 
                  Notification identifier of the first alarm to be
                  transmitted. All alarms with a notification identifier 
                  equal or greater than the specified notification identifier 
                  are transmitted.
                  The value 0 could be used for requesting the whole actual
                  content of the INCommander Active Alarm Database.
                   
                  Read access: 0"
        ::= { tiIncAlarmControl 12 }

        tiReqTransferIDs OBJECT-TYPE
                SYNTAX DisplayString
                MAX-ACCESS read-write
                STATUS current
                DESCRIPTION
                 "Reliable event transmission to an OS:

                  This object could be passed in a SNMP Set Request
                  along with the object tiOsName.0 in order to initiate
                  the retransmission of the specified alarm and alarm
                  cleared SNMP traps from the OS specific event buffer
                  to the requesting OS.

                  Write access: transfer identifier(s) of the event(s)
                  to be retransmitted to the requesting OS from the 
                  associated event buffer.
                  For the syntax refer to tiClearNotificationIds.

                  Read access: empty string."

        ::= { tiIncAlarmControl 20 }

        tiIncEventInfoTable OBJECT-TYPE
                SYNTAX  SEQUENCE OF EventInfoEntry
                MAX-ACCESS  not-accessible
                STATUS  current
                DESCRIPTION
                    "Reliable event transmission to an OS:
                     A list of tiIncEventInfoTable entries."
        ::= { tiIncAlarmControl 21 }

        tiIncEventInfoEntry OBJECT-TYPE
                SYNTAX     EventInfoEntry
                MAX-ACCESS not-accessible
                STATUS     current
                DESCRIPTION 
                    "Reliable event transmission to an OS:
                     This table provides information about the content 
                     of the event buffer for the requesting OS."
                INDEX { tiIncOsName }
        ::= { tiIncEventInfoTable 1 }

            EventInfoEntry ::= SEQUENCE {
                tiIncOsName            DisplayString(SIZE(0..32)),
                tiIncRecentTransferID  INTEGER (1..2147483647),
                tiIncOldestTransferID  INTEGER (1..2147483647)
        }

        tiIncOsName OBJECT-TYPE
                SYNTAX DisplayString(SIZE(0..32))
                MAX-ACCESS not-accessible
                STATUS current
                DESCRIPTION
                 "Reliable event transmission to an OS:
                  Symbolic name of the requesting OS in the 
                  INCommander object database."
        ::= { tiIncEventInfoEntry 1 }

        tiIncRecentTransferID OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION
                 "Reliable event transmission to an OS:
                  Transfer identifier of the most recent event available 
                  in the event buffer for the requesting an OS.
                  Could wrap from 2147483647 to 1."
        ::= { tiIncEventInfoEntry 2 }

        tiIncOldestTransferID OBJECT-TYPE
                SYNTAX INTEGER (1..2147483647)
                MAX-ACCESS read-only
                STATUS current
                DESCRIPTION
                 "Reliable event transmission to an OS:
                  Transfer identifier of the oldest event available 
                  in the event buffer for the requesting OS.
                  Could wrap from 2147483647 to 1."
        ::= { tiIncEventInfoEntry 3 }


        --
        -- Notifications Types :
        -- 

        -- notications types for single alarm and alarm cleared reports :

        tiIncTrapCritical NOTIFICATION-TYPE
                OBJECTS  { tiAlarmDateTime, 
                         tiAlarmReportingObject, 
                         tiAlarmFaultyObject, 
                         tiAlarmEventTypeId, 
                         tiAlarmSeverity, 
                         tiAlarmErrorID,
                         tiAlarmEndKey,
                         tiAlarmDescription,
                         tiAlarmSequenceNumber,
                         tiAlarmSourceName,
                         tiAlarmSymbNEname,
                         tiAlarmNotificationID,
                         tiAlarmTransferID
                }
                STATUS current
                DESCRIPTION 
                    "Trap issued by the INC to an OS with a severity 
                     value of 'critical'.
                     The objects contained in the variable binding list
                     are scalar objects ( with index 0 ).

                    SEVERITY-DEFAULT: Critical 
                    NOTIFICATIONID-VARIABLE: tiAlarmNotificationID
                    EVENTTYPE-VARIABLE: tiAlarmEventTypeId
                    ADDITIONAL-TEXT-VARIABLES: { tiAlarmDescription }
                    "
        ::= { tiIncTraps 2301 }
        
        tiIncTrapMajor NOTIFICATION-TYPE
                OBJECTS  { tiAlarmDateTime, 
                         tiAlarmReportingObject, 
                         tiAlarmFaultyObject, 
                         tiAlarmEventTypeId, 
                         tiAlarmSeverity, 
                         tiAlarmErrorID,
                         tiAlarmEndKey,
                         tiAlarmDescription,
                         tiAlarmSequenceNumber,
                         tiAlarmSourceName,
                         tiAlarmSymbNEname,
                         tiAlarmNotificationID,
                         tiAlarmTransferID
                }
                STATUS current
                DESCRIPTION 
                    "Trap issued by the INC to an OS with a severity 
                     value of 'major'.
                     The objects contained in the variable binding list
                     are scalar objects ( with index 0 ).

                    SEVERITY-DEFAULT: Major
                    NOTIFICATIONID-VARIABLE: tiAlarmNotificationID
                    EVENTTYPE-VARIABLE: tiAlarmEventTypeId
                    ADDITIONAL-TEXT-VARIABLES: { tiAlarmDescription }
                    "
        ::= { tiIncTraps 2302 }
        
        tiIncTrapMinor NOTIFICATION-TYPE
                OBJECTS  { tiAlarmDateTime, 
                         tiAlarmReportingObject, 
                         tiAlarmFaultyObject, 
                         tiAlarmEventTypeId, 
                         tiAlarmSeverity, 
                         tiAlarmErrorID,
                         tiAlarmEndKey,
                         tiAlarmDescription,
                         tiAlarmSequenceNumber,
                         tiAlarmSourceName,
                         tiAlarmSymbNEname,
                         tiAlarmNotificationID,
                         tiAlarmTransferID
                }
                STATUS current
                DESCRIPTION 
                    "Trap issued by the INC to an OS with a severity 
                     value of 'minor'.
                     The objects contained in the variable binding list
                     are scalar objects ( with index 0 ).

                    SEVERITY-DEFAULT: Minor
                    NOTIFICATIONID-VARIABLE: tiAlarmNotificationID
                    EVENTTYPE-VARIABLE: tiAlarmEventTypeId
                    ADDITIONAL-TEXT-VARIABLES: { tiAlarmDescription }
                    "
        ::= { tiIncTraps 2303 }
        
        tiIncTrapWarning NOTIFICATION-TYPE
                OBJECTS  { tiAlarmDateTime, 
                         tiAlarmReportingObject, 
                         tiAlarmFaultyObject, 
                         tiAlarmEventTypeId, 
                         tiAlarmSeverity, 
                         tiAlarmErrorID,
                         tiAlarmEndKey,
                         tiAlarmDescription,
                         tiAlarmSequenceNumber,
                         tiAlarmSourceName,
                         tiAlarmSymbNEname,
                         tiAlarmNotificationID,
                         tiAlarmTransferID
                }
                STATUS current
                DESCRIPTION 
                    "Trap issued by the INC to an OS with a 
                     severity value of 'warning'.
                     The objects contained in the variable binding list
                     are scalar objects ( with index 0 ).

                    SEVERITY-DEFAULT: Warning
                    NOTIFICATIONID-VARIABLE: tiAlarmNotificationID
                    EVENTTYPE-VARIABLE: tiAlarmEventTypeId
                    ADDITIONAL-TEXT-VARIABLES: { tiAlarmDescription }
                    "
        ::= { tiIncTraps 2304 }
        
        tiIncTrapNormal NOTIFICATION-TYPE
                OBJECTS  { tiAlarmDateTime, 
                         tiAlarmReportingObject, 
                         tiAlarmFaultyObject, 
                         tiAlarmEventTypeId, 
                         tiAlarmSeverity, 
                         tiAlarmErrorID,
                         tiAlarmEndKey,
                         tiAlarmDescription,
                         tiAlarmSequenceNumber,
                         tiAlarmSourceName,
                         tiAlarmSymbNEname,
                         tiAlarmNotificationID,
                         tiAlarmTransferID
                }
                STATUS current
                DESCRIPTION 
                    "Trap issued by the INC to an OS with a 
                     severity value of 'indeterminate'.
                     The objects contained in the variable binding list
                     are scalar objects ( with index 0 ).

                    SEVERITY-DEFAULT: Indeterminate
                    NOTIFICATIONID-VARIABLE: tiAlarmNotificationID
                    EVENTTYPE-VARIABLE: tiAlarmEventTypeId
                    ADDITIONAL-TEXT-VARIABLES: { tiAlarmDescription }
                    "
        ::= { tiIncTraps 2305 }
        
        tiIncTrapCleared NOTIFICATION-TYPE
                OBJECTS  { tiAlarmDateTime, 
                         tiAlarmReportingObject, 
                         tiAlarmFaultyObject, 
                         tiAlarmEventTypeId, 
                         tiAlarmSeverity, 
                         tiAlarmErrorID,
                         tiAlarmEndKey,
                         tiAlarmDescription,
                         tiAlarmSequenceNumber,
                         tiAlarmSourceName,
                         tiAlarmSymbNEname,
                         tiAlarmNotificationID,
                         tiAlarmTransferID
                }
                STATUS current
                DESCRIPTION 
                    "Trap issued by the INC to an OS with a 
                     severity value of 'cleared'.
                     The objects contained in the variable binding list
                     are scalar objects ( with index 0 ).

                    SEVERITY-DEFAULT: Clear
                    NOTIFICATIONID-VARIABLE: tiAlarmNotificationID
                    EVENTTYPE-VARIABLE: tiAlarmEventTypeId
                    ADDITIONAL-TEXT-VARIABLES: { tiAlarmDescription }
                    "
        ::= { tiIncTraps 2306 }


        -- notications types for alarm summary reports :

        tiIncPeriodicalSumRep NOTIFICATION-TYPE
                OBJECTS  { tiAlarmSourceName,
                           tiAlarmSymbNEname,
                           tiIsConnectionReliable,
                           tiMaxSeverity,
                           tiNumCriticalAlarms,
                           tiNumMajorAlarms,
                           tiNumMinorAlarms,
                           tiNumWarnings,
                           tiNumNormalEvents,
                           tiNumUnknownEvents
                }
                STATUS current
                DESCRIPTION 
                    "Periodical Alarm Summary Report for 
                     a single host."
        ::= { tiIncTraps 2310 }

        tiIncSpontaneousSumRep NOTIFICATION-TYPE
                OBJECTS  { tiAlarmSourceName,
                           tiAlarmSymbNEname,
                           tiIsConnectionReliable,
                           tiMaxSeverity,
                           tiNumCriticalAlarms,
                           tiNumMajorAlarms,
                           tiNumMinorAlarms,
                           tiNumWarnings,
                           tiNumNormalEvents,
                           tiNumUnknownEvents
                }
                STATUS current
                DESCRIPTION 
                    "Spontaneous Alarm Summary Report for 
                     a single host.
                     Will be issued if the value of the object
                     tiMaxSeverity has changed."
        ::= { tiIncTraps 2311 }

