log(DEBUG, "<<<<< Entering... cisco-CISCO-CABLE-QOS-MONITOR-MIB.adv.include.snmptrap.rules >>>>>")

switch($specific-trap)
{
    case "1": ### ccqmEnfRuleViolateNotification

        $OS_X733EventType = 1
        $OS_X733ProbableCause = 1008
        $OS_X733SpecificProb = "ccqmEnfRuleViolateNotification"
        $OS_OsiLayer = 0

        $OS_LocalPriObj = "ccqmEnfRuleViolateEntry." + $ifIndex + "." + $ccqmEnfRuleViolateID
        $OS_LocalRootObj = "ifEntry." + $ifIndex 
        $VAR_RelateLRO2LPO = 3
        $VAR_RelateLRO2LSO = 0
        $VAR_RelateLSO2LPO = 0

    default:
}

log(DEBUG, "<<<<< Leaving... cisco-CISCO-CABLE-QOS-MONITOR-MIB.adv.include.snmptrap.rules >>>>>")
